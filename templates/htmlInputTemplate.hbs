<form class="flexcol"></form>
    <div>
        <strong>Edit results</strong>
    </div>

    <div class="form-group stacked">
        <label class="form-label" for="value">Results for range {{{result.rangeText}}}</label><br/>
        <p>
        {{
            editor result.text
            target="text"
            button=false 
            editable=true 
            engine="prosemirror"
            collaborate=false 
        }}
        </p>
    </div>
    <footer class="form-footer">
        <input type="submit" value="Save" />
    </footer>
</form>